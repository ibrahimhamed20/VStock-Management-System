# Database Configuration
DATABASE_URL=postgresql://<DB_USER>:<DB_PASSWORD>@localhost:5432/<DB_NAME>
PG_HOST=localhost
PG_PORT=5432
PG_USER=<DB_USER>
PG_PASSWORD=<DB_PASSWORD>
PG_DATABASE=<DB_NAME>

# Ollama Configuration
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=llama3.1:8b

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=24h

# Email Configuration (for password reset, etc.)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# Application Configuration
PORT=3000
NODE_ENV=development

# AI Agent Configuration
VECTOR_STORE_TABLE=system_data
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
VECTOR_DIMENSIONS=384
SYNC_INTERVAL=300000 # 5 minutes in milliseconds 

# Cashing Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD= # Optional, if authentication is enabled

# Performance tuning environment variables
AI_SEARCH_RESULTS_LIMIT=15          # Number of search results
AI_MAX_CONTEXT_LENGTH=8000          # Maximum context length
AI_SYNC_INTERVAL_MINUTES=60         # Data sync interval
AI_CACHE_TTL=300                    # Cache TTL in seconds